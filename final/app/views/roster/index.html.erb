<h1>Roster List:</h1>

<table>
  <% @rosters = Roster.all %>
  <% @teams = Team.all %>
  <% @students = Student.all %>

  <% @rosters.each do |rost| %>
    <tr>
      <th>
        <%= rost.team.tName %>
      </th>
        <% rost.team.students.each do |student| %>
          <td>
            |<%= student.fname %> <%= student.lname %>
          </td>
        <% end %>
      <td>
        <a href = "/roster/<%=rost.id%>">show</a>
      </td>
    </tr>
  <% end %>
</table>
<br>
<%= link_to "add students teams", roster_newRost_path %>