<h1>Students</h1>

<table>
  <% @students.each do |student| %>
    <tr>
        <th>Fname</th>
        <th>Lname</th>
        <th>buckId</th>
        <th colspan="3"></th>
    </tr>
    <tr>
        <td><%= student.fname %></td>
        <td><%= student.lname %></td>
        <td><%= student.buckId %>></td>
    </tr>
  <% end %>
</table>
<br>
<%= link_to "new student",welcome_newStd_path%>