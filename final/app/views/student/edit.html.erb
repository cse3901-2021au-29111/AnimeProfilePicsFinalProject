<h1>Edit Student</h1>
<%=  form_with model: @student do |form| %>
  <div>
    <%= form.label :fname %><br>
    <%= form.text_field :fname %>
  </div>

  <div>
    <%= form.label :lname %><br>
    <%= form.text_field :lname %>
  </div>

  <div>
    <%= form.label :email %><br>
    <%= form.text_field :email %>
  </div>

  <div>
    <%= form.submit %>
  </div>

<% end %>

