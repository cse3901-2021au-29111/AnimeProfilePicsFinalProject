<h1>Students</h1>

<table>
  <% @students = Student.all %>
  <% @students.each do |student| %>
    <tr>
        <th>Fname</th>
        <th colspan="1"></th>
        <th>Lname</th>
        <th>Email</th>
        <th colspan="5"></th>
    </tr>
    <tr>
        <td><%= student.fname %></td>
        <td colspan="1"></th>
        <td><%= student.lname %></td>
        <td><%= student.email %></td>
        <td><a href = "/student/<%=student.id%>">show</td>
    </tr>
  <% end %>
</table>
<br>
<%= link_to "new student", student_newStd_path %>